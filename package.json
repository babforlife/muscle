  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/tailwindcss": "^5.3.2",
    "@vue/eslint-config-typescript": "^11.0.0",
    "core-js": "^3.25.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "eslint-plugin-vue": "^9.9.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.15.8",
    "prettier": "^2.8.3",
    "repo-check": "^1.29.0",
    "shuutils": "^7.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "description": "Muscle",
  "license": "MIT",
  "name": "muscle",
  "private": true,
  "scripts": {
    "build": "nuxt generate",
    "check": "repo-check",
    "ci": "run-s build lint test",
    "dev": "nuxt",
    "lint": "run-s lint:*",
    "lint:js": "eslint --fix --ignore-path .gitignore --ext .js,.ts,.html,.vue .",
    "lint:css": "stylelint \"{assets,components,layouts,pages}/**/*.{css,vue}\" --fix",
    "postversion": "git push && git push --tags",
    "preversion": "npm run ci",
    "start": "npm run dev",
    "test": "",
    "test:watch": "nodemon --quiet --watch services --watch tests --exec npm test --ext .ts",
    "update": "npx npm-check-updates -u"
  },
  "version": "0.0.0"
}